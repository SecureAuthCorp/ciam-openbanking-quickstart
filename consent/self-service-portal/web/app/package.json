{
  "name": "consent-self-service-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:8085",
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.15.0",
    "@mui/lab": "5.0.0-alpha.156",
    "@mui/material": "5.15.0",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.5",
    "@types/ramda": "0.29.9",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "fast-text-encoding": "1.0.6",
    "lodash": "4.17.21",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.21.0",
    "react-router-dom": "6.21.0",
    "superagent": "8.1.2",
    "tss-react": "4.9.3",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.1.1",
    "react-scripts": "5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "react/jsx-curly-brace-presence": [
        "error",
        {
          "props": "never",
          "children": "never"
        }
      ],
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            "@mui/icons-material",
            "@mui/material",
            "@mui/lab",
            "lodash"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
