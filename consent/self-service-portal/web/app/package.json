{
  "name": "consent-self-service-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:8085",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.97",
    "@mui/material": "^5.10.3",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "@types/ramda": "^0.28.15",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "fast-text-encoding": "^1.0.6",
    "lodash": "^4.17.21",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "superagent": "^8.0.0",
    "tss-react": "^4.0.0",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "react/jsx-curly-brace-presence": [
        "error",
        {
          "props": "never",
          "children": "never"
        }
      ],
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            "@mui/icons-material",
            "@mui/material",
            "@mui/lab",
            "lodash"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
